create database employee;
USE employee;
create TABLE Employee(ssn int primary key, emp_name varchar(20), age int default 25);
insert into Employee values(1201, 'Tharuni', 22);
insert into Employee values(1202, 'Shivani', 18);
insert into Employee values(1203, 'Ramya', null);
insert into Employee(ssn, emp_name) values (1204, 'Sathvika');
create TABLE Student(roll_num int, name varchar(25), check(roll_num>0));
insert into Student values(1, 'Shivani');
select * from Student;
select * from Employee;
create TABLE Student1(ht_num int primary key, name varchar(20));
insert into Student1 values(1201, 'Harshitha'), (1202, 'Vani'), (1203, 'Sindhu');
create TABLE Event(event_id int primary key, event_name varchar(15), coord_id int, foreign key(coord_id) references Student1(ht_num));
insert into Event values(101, 'dancing', 1203), (102, 'sports', 1201), (103, 'drawing', 1202);
select * from Event;
select * from Student1;
show tables;
